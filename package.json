{
  "name": "dead-code-checker",
  "version": "0.0.9",
  "description": "Dead code checker",
  "license": "MIT",
  "repository": "https://github.com/denisoed/dead-code-checker",
  "homepage": "https://github.com/denisoed/dead-code-checker",
  "bugs": "https://github.com/denisoed/dead-code-checker/issues",
  "author": {
    "name": "Denisoed",
    "email": "denisoed@gmail.com",
    "url": "https://denisoed.com"
  },
  "exports": {
    ".": {
      "types": "./dist/types",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/types",
  "files": [
    "dist"
  ],
  "bin": {
    "dead-code-checker": "./bin/index.js"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:types && rollup -c",
    "make": "npm run build && node ./example/index.js",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublish": "npm run build && npm run test",
    "format": "prettier \"src/**/*.ts\" --write --config .prettierrc",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/register": "^7.23.7",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "cfonts": "^3.2.0",
    "colors": "^1.4.0",
    "commander": "^12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "es5",
    "react",
    "vue",
    "svelte",
    "nuxt",
    "next",
    "angular",
    "vanilla",
    "jquery",
    "plugins",
    "analyzer",
    "checker",
    "html",
    "javascript"
  ]
}
